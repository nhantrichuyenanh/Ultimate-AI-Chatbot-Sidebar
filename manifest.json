{
  "manifest_version": 3,
  "name": "Ultimate AI Chatbot Sidebar",
  "description": "Quickly access and manage your favorite AI chatbots.",
  "version": "1.0.0",
  "browser_specific_settings": {
    "gecko": {
      "id": "experiment@email.com",
      "strict_min_version": "109.0"
    }
  },
  "sidebar_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "96": "icons/icon96.png"
    },
    "default_title": "AI Chatbot Sidebar",
    "default_panel": "sidebar/sidebar.html",
    "open_at_install": true
  },
  "page_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png"
    },
    "default_title": "Toggle Ultimate AI Chatbot Sidebar"
  },
  "background": {
    "scripts": [
      "domains.js",
      "background.js"
    ]
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "96": "icons/icon96.png"
  },
  "permissions": [
    "storage"
  ],
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": false
  }
}